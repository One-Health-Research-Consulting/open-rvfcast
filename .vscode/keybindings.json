// Place your key bindings in this file to override the defaults
[
    {
      "key": "cmd+shift+n",
      "command": "foam-vscode.create-note"
    },
    {
      "key": "cmd+shift+d",
      "command": "foam-vscode.open-daily-note"
    },

    {
      "command": "runCommands",
      "key": "shift+cmd+r",
      "args": {
        "commands": [
          // commands to run in sequence
          "r.createRTerm",
          "workbench.action.terminal.moveToEditor"
        ]
      }
    }
]
